	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/application_5.xsd"
	version="5">

	<!-- Don't miss ending '/' for archived war file. -->
	<module>
		<web>
			<web-uri>/webapp/web.war</web-uri>
			<context-root>/</context-root>
		</web>
	</module>

	<module>
		<ejb>ejb/nexcore-framework-core-6.5.0.1-services.jar</ejb>
	</module>

	<module>
		<ejb>ejb/nexcore.framework.bat.jar</ejb>
	</module>
</application>